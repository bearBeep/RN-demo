{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Native",
  "_version": "2.0.0",
  "compilerOptions": {
    "jsx": "react-native",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": "./src",
    "paths": {
      "@components": ["components/*"],
      "@context": ["context/*"],
      "@pages": ["pages/*"],
      "@routes": ["routes/*"]
    }
  }
}
